SELECT DISTINCT
sent_month,
id_account,
msg_cnt_account /
msg_cnt_month *100 AS sent_msg_percent_from_this_month,
first_sent_date,
last_sent_date
FROM `Students.v_cnt_main` AS cnt_main

CREATE VIEW `Students.v_cnt_main` AS(
SELECT
DATE_TRUNC(DATE_ADD(s.date, INTERVAL es.sent_date DAY), MONTH) AS sent_month,
es.id_account,
count(es.id_message) over (partition by es.id_account, DATE_TRUNC(DATE_ADD(s.date, INTERVAL es.sent_date DAY), MONTH)) as msg_cnt_account,
count(es.id_message) over (partition by DATE_TRUNC(DATE_ADD(s.date, INTERVAL es.sent_date DAY), MONTH)) as msg_cnt_month,
MIN(DATE_ADD(s.date,INTERVAL es.sent_date DAY)) OVER (PARTITION BY es.id_account, DATE_TRUNC(DATE_ADD(s.date, INTERVAL es.sent_date DAY), MONTH))AS first_sent_date,
MAX(DATE_ADD(s.date,INTERVAL es.sent_date DAY)) OVER (PARTITION BY es.id_account, DATE_TRUNC(DATE_ADD(s.date, INTERVAL es.sent_date DAY), MONTH))AS last_sent_date,
FROM `DA.account_session` acs
JOIN `DA.session` s
ON acs.ga_session_id = s.ga_session_id
JOIN `DA.email_sent` es
ON acs.account_id = es.id_account
)
